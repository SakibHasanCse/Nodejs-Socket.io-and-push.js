 <div class="row">
      <div class="col s12 m7">
        <% users.forEach(function(img) { %>
        <div class="card">
          <div class="card-image">
            <img src="<%=img.profileimage%>" />
          </div>
          <div class="card-content">
            <h5><%= img.name %></h5>

            <p><%= img.email %></p>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
    